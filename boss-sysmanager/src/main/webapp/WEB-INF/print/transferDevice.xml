<table width="1000px" border="0" align="left">
  <tr>
    <td height="88px" colspan="8" align="center"><table width="100%" height="100%" border="0">
      <tr>
		<td height="10px" colspan="8" align="center" class="t1">Cambodian Superme Telecommunication Media Group Co.,ltd</td>
	  </tr>
	  <tr>
		<td height="10px" colspan="8" align="center" class="t1">柬埔寨通信传媒集团有限公司</td>
	  </tr>
	  <tr>
		<td height="10px" colspan="8" align="center" class="t2">OUTGOING MATERIAL</td>
	  </tr>
	  <tr>
		<td height="10px" colspan="8" align="center" class="t2">物料出库单</td>
	  </tr>
    </table></td>
  </tr>
  <tr class="t3">
    <td height="5px" colspan="2" align="right" >RequestionDepartment</td>
    <td width="190px" rowspan="2" align="left" valign="middle">{[values.transferDevice.depot_order_text || '']}</td>
    <td width="60px" align="right">Operator</td>
    <td width="160px" rowspan="2" align="left" valign="middle">{[values.transferDevice.optr_name || '']}</td>
    <td width="100px" align="right">Date</td>
    <td width="220px" rowspan="2" align="left" valign="middle">{[fm.dateFormatHIS(values.transferDevice.create_time)]}</td>
  </tr>
  <tr class="t3">
    <td height="5px" width="40px" align="center">&nbsp;</td>
    <td width="150px" align="right">申请部门</td>
    <td align="right">操作员</td>
    <td align="right">出库日期</td>
  </tr>
  <tr>
  	<td height="1px" colspan="8"></td>
  </tr>
  <tr>
    <td colspan="8" align="center"><table width="100%" height="100%" border="0" cellpadding="0" cellspacing="0" class="t4">
      <tr>
		<td height="30px" width="5%" align="center" class="tdtlr">No.</td>
		<td width="25%" align="center" class="tdtlr">Specification</td>
		<td width="17%" align="center" class="tdtlr">Batch No.</td>
		<td width="15%" align="center" class="tdtlr">Box No.</td>
		<td width="13%" align="center" class="tdtlr">Supplier</td>
		<td width="6%" align="center" class="tdtlr">Unit</td>
		<td width="8%" align="center" class="tdtlr">Amount</td>
		<td width="11%" align="center" class="tdtlr">Remark</td>
	  </tr>
	  <tr>
		<td height="30px" align="center" class="tdrbl">序号</td>
		<td align="center" class="tdrbl">规格型号</td>
		<td align="center" class="tdrbl">批次号</td>
		<td align="center" class="tdrbl">纸箱号</td>
		<td align="center" class="tdrbl">供应商</td>
		<td align="center" class="tdrbl">单位</td>
		<td align="center" class="tdrbl">数量</td>
		<td align="center" class="tdrbl">备注</td>
	  </tr>
	  <tpl for="values.deviceListInfo">
		 <tr height="60px">
		 	<td align="center" class="tbsolid">{[xindex]}</td> 
		 	<td align="center" class="tbsolid">{[values.model_name || '']}</td> 
		 	<td align="center" class="tbsolid">{[values.batch_num || '']}</td> 
		 	<td align="center" class="tbsolid">{[values.box_no || '']}</td> 
		 	<td align="center" class="tbsolid">{[values.supplier_name || '']}</td> 
		 	<td align="center" class="tbsolid">个</li> 
		 	<td align="center" class="tbsolid">{[values.total_num || '']}</td> 
		 	<td align="center" class="tbsolid">备注</td>
		 </tr>
	 </tpl>
    </table></td>
  </tr>
  <tr>
    
    <tpl if="values.deviceListInfo.length == 4">
    	<td height="60px" colspan="8" align="center">&nbsp;</td>
    </tpl>
    <tpl if="values.deviceListInfo.length != 4">
    	<td height="{[(4-values.deviceListInfo.length)*60 + 60]}px" colspan="8" align="center">&nbsp;</td>
    </tpl>
  </tr>
  <tr>
    <td height="20px" colspan="8" align="center"><table width="100%" border="0" class="t2">
      <tr>
        <td width="19%" align="right">Taker</td>
        <td width="1%" rowspan="2" valign="middle">:</td>
        <td width="43%" rowspan="2" valign="middle">&nbsp;</td>
        <td width="10%" align="right">Keeper</td>
        <td width="2%" rowspan="2" valign="middle">:</td>
        <td width="25%">&nbsp;</td>
      </tr>
      <tr>
        <td align="right">出库领用人</td>
        <td align="right">仓管员</td>
        <td>&nbsp;</td>
      </tr>
    </table></td>
  </tr>
</table>
